{
    "workbench.startupEditor": "newUntitledFile",
    "typescript.tsserver.useSeparateSyntaxServer": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "terraform.languageServer": {
        "external": true,
        "args": [
            "serve"
        ]
    },
    "[terraform]": {
        "editor.formatOnSave": true
    },
    "terminal.integrated.rendererType": "dom",
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "files.autoSave": "off",
    // go
    "go.lintTool":"golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "go.formatTool": "goimports",
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true
    },
    "go.useLanguageServer": true,
    "go.toolsManagement.autoUpdate": true,
    "go.goroot": "",
    "hediet.vscode-drawio.local-storage": "",
    "hediet.vscode-drawio.offline": false,
    // python
    "[python]": {
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ]
    },
    "python.pythonPath": "/Users/kenzo.tanaka/.asdf/shims/python",
    // python - formatter
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--skip-string-normalization"
    ],
    "python.languageServer": "Pylance",
    // python - linter
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--max-line-length 119",
        "--max-complexity=20"
    ],
    "python.linting.mypyEnabled": true,
    "python.linting.banditEnabled": true,
    "python.linting.lintOnSave": true,
    "editor.formatOnPaste": false,
    "diffEditor.ignoreTrimWhitespace": false,
    // javascript
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "eslint.alwaysShowStatus": true,
    "eslint.codeAction.showDocumentation": {
        "enable": true
    },
    "eslint.packageManager": "yarn",
    "debug.javascript.autoAttachFilter": "always",
    "eslint.format.enable": true,
    "editor.formatOnSave": true,
    "prettier.tabWidth": 2,
    "prettier.jsxSingleQuote": true,
    "eslint.run": "onSave",
    "eslint.nodePath": "",
    "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
    "editor.tokenColorCustomizations": null,
    "python.defaultInterpreterPath": "/Users/kenzo.tanaka/.asdf/shims/python",
    "json.schemas": [],
    "aws.profile": "profile:default",
    "prettier.singleQuote": true,
    "extensions.ignoreRecommendations": true
}
